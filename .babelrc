{
  "presets": [
		"@babel/preset-env",
		"@babel/preset-react",
		"@babel/preset-typescript"
	],
  "plugins": [
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "esm": {
      "presets": [["@babel/preset-env", { "modules": false }]],
      "plugins": ["@babel/plugin-proposal-class-properties", ["@babel/plugin-transform-runtime", { "useESModules": true }]]
    }
  }
}